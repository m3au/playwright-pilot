{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["-y", "@ejazullah/mcp-playwright"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@missionsquad/mcp-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    }
  }
}
